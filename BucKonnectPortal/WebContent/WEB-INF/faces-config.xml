<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
	<managed-bean>
		<managed-bean-name>welcomeBean</managed-bean-name>
		<managed-bean-class>backingBeans.WelcomeBean</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>registerBean</managed-bean-name>
		<managed-bean-class>backingBeans.RegisterBean</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>loginBean</managed-bean-name>
		<managed-bean-class>backingBeans.LoginBean</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>homeBean</managed-bean-name>
		<managed-bean-class>backingBeans.HomeBean</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
		<managed-property>
			<property-name>loginBean</property-name>
			<property-class>backingBeans.LoginBean</property-class>
			<value>#{loginBean}</value>
		</managed-property>
		<managed-property>
			<property-name>userProfileBean</property-name>
			<property-class>backingBeans.UserProfileBean</property-class>
			<value>#{userProfileBean}</value>
		</managed-property>
		<managed-property>
			<property-name>searchUserGroupBean</property-name>
			<property-class>backingBeans.SearchUserGroupBean</property-class>
			<value>#{searchUserGroupBean}</value>
		</managed-property>
		<managed-property>
			<property-name>searchUserEventBean</property-name>
			<property-class>backingBeans.SearchUserEventBean</property-class>
			<value>#{searchUserEventBean}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>createGroupBean</managed-bean-name>
		<managed-bean-class>backingBeans.CreateGroupBean</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
		<managed-property>
			<property-name>loginBean</property-name>
			<property-class>backingBeans.LoginBean</property-class>
			<value>#{loginBean}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>createEventBean</managed-bean-name>
		<managed-bean-class>backingBeans.CreateEventBean</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
		<managed-property>
			<property-name>loginBean</property-name>
			<property-class>backingBeans.LoginBean</property-class>
			<value>#{loginBean}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>userProfileBean</managed-bean-name>
		<managed-bean-class>backingBeans.UserProfileBean</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
		<managed-property>
			<property-name>loginBean</property-name>
			<property-class>backingBeans.LoginBean</property-class>
			<value>#{loginBean}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>searchEventBean</managed-bean-name>
		<managed-bean-class>backingBeans.SearchEventBean</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>searchGroupBean</managed-bean-name>
		<managed-bean-class>backingBeans.SearchGroupBean</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>searchUserBean</managed-bean-name>
		<managed-bean-class>backingBeans.SearchUserBean</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>searchUserGroupBean</managed-bean-name>
		<managed-bean-class>backingBeans.SearchUserGroupBean</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
		<managed-property>
			<property-name>loginBean</property-name>
			<property-class>backingBeans.LoginBean</property-class>
			<value>#{loginBean}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>searchUserEventBean</managed-bean-name>
		<managed-bean-class>backingBeans.SearchUserEventBean</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
		<managed-property>
			<property-name>loginBean</property-name>
			<property-class>backingBeans.LoginBean</property-class>
			<value>#{loginBean}</value>
		</managed-property>
	</managed-bean>
	<navigation-rule>
		<display-name>CreateGroup.xhtml</display-name>
		<from-view-id>/CreateGroup.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Success</from-outcome>
			<to-view-id>/GroupCreated.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>Error</from-outcome>
			<to-view-id>/Error.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>CreateEvent.xhtml</display-name>
		<from-view-id>/CreateEvent.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Success</from-outcome>
			<to-view-id>/EventCreated.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>Error</from-outcome>
			<to-view-id>/Error.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>SearchEvent.xhtml</display-name>
		<from-view-id>/SearchEvent.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Success</from-outcome>
			<to-view-id>/EventsFound.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>Error</from-outcome>
			<to-view-id>/Error.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>SearchGroup.xhtml</display-name>
		<from-view-id>/SearchGroup.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Success</from-outcome>
			<to-view-id>/GroupsFound.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>Error</from-outcome>
			<to-view-id>/Error.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>SearchUser.xhtml</display-name>
		<from-view-id>/SearchUser.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Success</from-outcome>
			<to-view-id>/UsersFound.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>Error</from-outcome>
			<to-view-id>/Error.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>UsersFound.xhtml</display-name>
		<from-view-id>/UsersFound.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/Home.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>GroupsFound.xhtml</display-name>
		<from-view-id>/GroupsFound.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/Home.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>EventsFound.xhtml</display-name>
		<from-view-id>/EventsFound.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/Home.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Login.xhtml</display-name>
		<from-view-id>/Login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>register</from-outcome>
			<to-view-id>/Register.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/Home.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>Error</from-outcome>
			<to-view-id>/Error.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Register.xhtml</display-name>
		<from-view-id>/Register.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/Login.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>Error</from-outcome>
			<to-view-id>/Error.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Home.xhtml</display-name>
		<from-view-id>/Home.xhtml</from-view-id>
	</navigation-rule>
	<navigation-rule>
		<display-name>Home.xhtml</display-name>
		<from-view-id>/Home.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>UserProfile</from-outcome>
			<to-view-id>/Profile.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>UserGroups</from-outcome>
			<to-view-id>/MyGroups.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>UserEvents</from-outcome>
			<to-view-id>/MyEvents.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Profile.xhtml</display-name>
		<from-view-id>/Profile.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/Home.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/Home.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>Error</from-outcome>
			<to-view-id>/Error.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/MyGroups.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Error</from-outcome>
			<to-view-id>/Error.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<application />
</faces-config>
